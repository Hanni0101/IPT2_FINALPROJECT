{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/package.json",
      "use": "@vercel/static-builds",
      "config": {
        "distDir": "dashboard/dist"
      }
    },
    {
      "src": "server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "dashboard/dist/index.html"
    }
  ]
}
