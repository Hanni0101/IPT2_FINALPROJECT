{
  "version": 2,
  "buildCommand": "npm --prefix dashboard install && npm --prefix dashboard run build",
  "outputDirectory": "dashboard/dist",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api/index.js"
    },
    {
      "src": "^/(.*)$",
      "dest": "/dashboard/dist/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/dashboard/dist/index.html"
    }
  ]
}
